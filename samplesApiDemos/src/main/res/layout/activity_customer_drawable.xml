<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <com.example.android.apis.mydemo.drawable.CustomerRectangleView
        android:layout_width="100dp"
        android:layout_height="46dp" />

    <com.example.android.apis.mydemo.drawable.CustomerCircleRectangleView
        android:layout_width="100dp"
        android:layout_height="46dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="8"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="自定义selector"
        android:textColor="#000000"
        android:textSize="18sp" />

    <com.example.android.apis.mydemo.drawable.ImageCircleView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="10dp" />


</LinearLayout>